{{- if ne .chezmoi.osRelease.id "ubuntu" -}}
[general]
{{- end }}
import = [
"keybindings.toml",
"flexoki-dark.toml",
]

[env]
TERM = "xterm-256color"

[font]
size = 16.0

[font.normal]
family = "JetBrains Mono"

[font.offset]
y = 1

[window]
startup_mode = "Fullscreen"

[window.padding]
x = 8
y = 4
